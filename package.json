{
  "name": "nextjs-template",
  "version": "1.0.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "private": true,
  "standard-version": {
    "scripts": {
      "prebump": "nfqBump"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "template": {
    "version": "2.0.0",
    "name": "nextjs-typescript-template",
    "path": "https://github.com/nfqde/nextjs-typescript-template.git"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=true FEATURE_ENV=live next build",
    "analyze:browser": "cross-env ANALYZE=true BUNDLE_ANALYZE=browser FEATURE_ENV=live next build",
    "analyze:server": "cross-env ANALYZE=true BUNDLE_ANALYZE=server FEATURE_ENV=live next build",
    "build:live": "cross-env NODE_ENV=production FEATURE_ENV=live next build",
    "build:stage": "cross-env NODE_ENV=production FEATURE_ENV=stage next build",
    "build:test": "cross-env NODE_ENV=production FEATURE_ENV=test next build",
    "check:types": "tsc -p .",
    "deploy": "standard-version && git push --follow-tags origin main",
    "dev": "python -t -m webbrowser http://localhost:3000 && cross-env FEATURE_ENV=stage pnpm next dev -p 3000",
    "lint": "eslint ./src --ext='.js' --ext='.jsx' --ext='.ts' --ext='.tsx'",
    "lint:report": "eslint --output-file eslint_report.json --format json --ext='.js, .jsx, .ts, .tsx' ./src",
    "merge:files": "pnpm mochawesome-merge \"./cypress/reports/mochawesome*.json\" > \"./cypress/reports/index.json\"",
    "start": "cross-env NODE_ENV=production next start -p 3000",
    "start:test": "start-server-and-test 'pnpm run start' http://localhost:3000 'pnpm run test'",
    "start:test:merge": "start-server-and-test 'pnpm run start' http://localhost:3000 'pnpm run test:merge'",
    "test:open:cypress": "cross-env NODE_ENV=test pnpm cypress open",
    "test:cypress:ct": "cross-env NODE_ENV=test pnpm cypress run --component && pnpm merge:files",
    "test:cypress:integration": "cross-env NODE_ENV=test pnpm cypress run",
    "test:merge": "pnpm run test:cypress:integration && pnpm run test:cypress:ct && pnpm nfqReport",
    "test": "pnpm run test:cypress:integration && pnpm run test:cypress:ct",
    "generate:types": "pnpm nfq-generate-types -f \"@app/features\" -j \"@nfq/feature-flags/jsx\" -i \"WithFeature\" -x \"WithoutFeature\" -e stage"
  },
  "dependencies": {
    "@nfq/next-fonts": "^1.3.2",
    "@nfq/react-grid": "^3.1.2",
    "@nfq/typed-next-api": "^1.4.7",
    "core-decorators": "^0.20.0",
    "framer-motion": "^10.16.1",
    "mobx": "^6.10.0",
    "mobx-react-lite": "^4.0.3",
    "next": "^13.4.19",
    "next-connect": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.11",
    "swr": "^2.2.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.7.3",
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@babel/plugin-transform-typescript": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@cypress/code-coverage": "^3.11.0",
    "@next/bundle-analyzer": "^13.4.19",
    "@next/env": "^13.4.19",
    "@nfq/eslint-config": "^3.0.3",
    "@nfq/eslint-plugin": "^0.6.1",
    "@nfq/feature-flags": "^3.0.3",
    "@nfq/test-reporter": "^1.1.5",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/node": "^20.5.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-styled-components": "^2.1.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^7.0.3",
    "cypress": "^12.17.4",
    "cypress-real-events": "^1.10.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^8.47.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-array-func": "^3.1.8",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.5.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-perf-standard": "^1.0.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-hooks-ssr": "^0.1.5",
    "eslint-plugin-redos": "4.5.0-beta.2",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "html-webpack-plugin": "^5.5.3",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "nyc": "^15.1.0",
    "onecolor": "^4.0.0",
    "react-is": "^18.2.0",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}